<mjml>
  <mj-head>
    <mj-preview>Take the survey with BROFIST</mj-preview>
    <mj-title>Take the survey with BROFIST</mj-title>
    <mj-attributes>
      <mj-body></mj-body>
      <mj-section padding="0px"></mj-section>
      <mj-column padding="0px" vertical-align="middle"></mj-column>
      <mj-wrapper padding="0px"></mj-wrapper>
      <mj-image padding="0" align="center" fluid-on-mobile="true" css-class="img"></mj-image>
      <mj-text align="center" font-family="verdana" line-height="18px"></mj-text>
    </mj-attributes>
    <mj-style>

      a { text-decoration: none;}
      img {max-width:100% !important;}
      
      .icon table tbody tr td img {width:auto !important; } 
      .statistics_TTT table tbody tr td img {width:auto !important; } 
      .centered_img table tbody tr td {text-align: center !important; max-width:100%;} 
      .products table {text-align:center; border:1px solid red;} 
      .products table tbody tr td {padding:5px !important;} 
      .center table tbody tr td{ text-align: center !important; } 
      .highlight { padding:0 0 5px 5px; background-image: url('https://raw.githubusercontent.com/FrankyMelon/email_example/main/font_highlight.png'); background-size: cover; background-repeat: no-repeat; background-position-y: 5px; background-position-x: -5px;}
      
    </mj-style>
  </mj-head>
  <mj-body background-color="#FFFFFF" width="600px">
    <!-- HEADER -->
    <mj-section css-class="width_auto" padding="22px 5%" background-url="https://raw.githubusercontent.com/FrankyMelon/email_example/main/header_bgr.png" background-repeat="no-repeat" background-size="contain">
      <mj-group>
        <mj-column width="76%">
          <mj-raw>

            <a href="https://brofist.partners/" title="Visit Brofist site" aria-label="Link to Brofist site"><img width="168px" align="left" alt="Brofist logo" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/brofist_logo.png" /></a>
          </mj-raw>
          </mj-image>
        </mj-column>
        <mj-column width="12%">
          <mj-raw>
            <a href="https://join.skype.com/invite/BQyqFGHExgjx" title="Invite to Skype chat" aria-label="Invite to Skype chat"><img width="43px" alt="Skype icon" style="padding-right: 40px;" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/skype.png" /></a>
          </mj-raw>
        </mj-column>
        <mj-column width="12%">
          <mj-raw>
            <a href="https://t.me/Alina_Brofist" title="Invite to Telegram chat" aria-label="Invite to Telegram chat"><img width="43px" alt="Telegram icon" style="padding: 0 32px 0 10px;" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/Telegram.png" /></a>
          </mj-raw>
        </mj-column>
      </mj-group>
    </mj-section>
    <!-- PERSONALIZED -->
    <mj-section padding="0 48px">
      <mj-column>
        <mj-spacer height="50px"></mj-spacer>
        <mj-image title="Your personalized and 500 EUR to the first payout is almost here. Just take a survey. Get it" alt="Your personalized and 500 EUR to the first payout is almost here. Just take a survey. Get it" href="https://brofistpartners.com/kviz" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/personalized_bgr.png" />
      </mj-column>
    </mj-section>
    <!-- TEXT -->
    <mj-section padding="36px 0 35px 0">
      <mj-column>
        <mj-text font-weight="bold" font-size="28px" line-height="28px">Do you want to know more?</mj-text>
        <mj-text font-size="14px" letter-spacing="0.5px" padding="19px 18% 0 18%">The one who clicks the button gets such results in just 14 days:</mj-text>
      </mj-column>
    </mj-section>
    <!-- STATISTICS -->
  
  <!-- Part 1 -->
    <mj-section padding="36px 0 0 0">
      <mj-group>
        <mj-column vertical-align="top" width="6%">
          <mj-raw>
            <img aria-hidden="true" width="28px" style="padding:0 6px 0 0;" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/bgr_cubes_LT.png" />
          </mj-raw>
        </mj-column>
        <mj-column css-class="centered_img" width="88%">
          <mj-raw>
            <img alt="Images describes statistics and reports in table view" style="text-align:center; padding:30px 5px 0 5px;" width="469px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/statistics.png" />
          </mj-raw>
          <mj-text>It can still be yours! <span class="highlight">Just take the survey.</span></mj-text>
        </mj-column>
        <mj-column vertical-align="top" width="6%">
          <mj-raw>
            <img aria-hidden="true" width="28px" style="padding:0 0 0 6px ;" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/bgr_cubes_RT.png" />
          </mj-raw>
        </mj-column>
      </mj-group>
    </mj-section>
  
<!-- Part 2 -->  
    <mj-section padding="22px 0 0 0">
      <mj-group>
        <mj-column vertical-align="bottom" width="6%">
          <mj-raw>
            <img aria-hidden="true" width="28px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/bgr_cubes_LT.png" />
          </mj-raw>
        </mj-column>
     <mj-column css-class="centered_img" width="88%">
        <mj-raw>
          <a href="https://brofistpartners.com/kviz" title="Take the survey button" aria-label="Take the survey button"><img alt="Take the survey button" width="198px" style="padding-bottom:30px; text-align:center;" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/take_survey.png" /></a>
        </mj-raw>

      </mj-column>
        <mj-column vertical-align="bottom" width="6%">
          <mj-raw>
            <img aria-hidden="true" width="28px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/bgr_cubes_RT.png" />
          </mj-raw>
        </mj-column>
        </mj-group>
    </mj-section>
    <!-- By the way-->
    <mj-section padding="35px 0 0 0" css-class="PRODUCTS_section">
      <mj-column css-class="PRODUCTS_column">
        <mj-text font-size="20px" font-weight="bold" padding-bottom="7px">By the way, </mj-text>
        <mj-text font-size="20px" font-weight="bold" padding-top="7px"> here are our three products: </mj-text>
      </mj-column>
    </mj-section>
  
  <!-- PRODUCTS-->
    <mj-section padding="81px 0 0 0 ">
      <mj-column css-class="products">
        <mj-raw>
          <img alt="Dazard casino icon" width="127px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/dazard.png" />
        </mj-raw>
      </mj-column>
      <mj-column css-class="products">
        <mj-raw>
          <img alt="Millionaria casino icon" width="127px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/millionaria.png" />
        </mj-raw>
      </mj-column>
      <mj-column css-class="products">
        <mj-raw>
          <img alt="Billy Billion icon" width="127px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/billy_billion.png" />
        </mj-raw>
      </mj-column>
    </mj-section>
  
  <!-- Conversion up -->
    <mj-section padding="16px 0 0 0">
      <mj-column>
        <mj-text padding="10px 0 6px 0">Conversion up <span class="highlight">to 70%</span></mj-text>
        <mj-text padding="0"><span class="highlight">Avalible to</span> CH, AU, AT, DK, IE </mj-text>
        <mj-spacer height="40px" />
      </mj-column>
    </mj-section>
  
    <!--FOOTER-->
  
<mj-section padding="56px 0">
      <mj-group>
        <mj-column vertical-align="middle" width="10%">
          <mj-raw>
            <img aria-hidden="true" width="58px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/footer_l.png" />
          </mj-raw>
        </mj-column>
     <mj-column css-class="centered_img" width="80%">
        <mj-raw>
          <a href="https://brofist.partners/" title="Visit Brofist site" aria-label="Link to Brofist site"><img alt="Brofit logo" width="127px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/brofist_logo.png" /></a>
        </mj-raw>
      </mj-column>
        <mj-column vertical-align="middle" width="10%">
          <mj-raw>
            <img aria-hidden="true" width="58px" src="https://raw.githubusercontent.com/FrankyMelon/email_example/main/footer_r.png" />
          </mj-raw>
        </mj-column>
        </mj-group>
    </mj-section>  
  
  
  
  
  
  
  
  
  
